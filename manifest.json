{
  "name": "豆瓣读书-美团馆藏查询",
  "version": "2.2.0",
  "manifest_version": 2,
  "description": "search books on douban.com more conveniently",
  "background": {
        "scripts": ["js/search.js"]
    },
  "content_scripts": [
  {
      "matches": ["http://book.douban.com/subject/*"],
      "js": ["js/jquery.js","js/public.js","js/parse_book.js","js/query.js"]
  },
  {
      "matches": ["http://book.douban.com/people/**wish*"],
      "js": ["js/jquery.js","js/public.js","js/douban.js"]
  }
  ],
  "permissions": [
	"contextMenus","tabs", "http://*/*","https://*/*"  
  ]
}
