{
  "name": "Douban Mashuper",
  "version": "1.1",
  "description": "It makes more Doubaner from UESTC happier.",
  "background_page": "bg.html",
  "browser_action": {
    "default_title": "Lib of UESTC",
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
 "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["http://book.douban.com/subject/*"],
      "css": ["style.css"],
      "js": ["js/jquery.js","js/parse_book.js","js/recommend.js","js/query.js"]
    }
  ],
  "permissions": [
 "tabs", "http://*/*","https://*/*"  
  ]
}
